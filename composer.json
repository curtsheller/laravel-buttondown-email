{
    "name": "juststeveking/laravel-buttondown-email",
    "description": "Manage Newsletters in Laravel, using the buttondown.email API",
    "keywords": [
        "juststeveking",
        "laravel-buttondown-email",
        "api",
        "integration",
        "buttondown email"
    ],
    "type": "library",
    "homepage": "https://github.com/juststeveking/laravel-buttondown-email",
    "license": "MIT",
    "authors": [
        {
            "role": "Developer",
            "name": "Steve McDougall",
            "email": "juststevemcd@gmail.com",
            "homepage": "https://www.juststeveking.uk/"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.41",
        "juststeveking/uri-builder": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "JustSteveKing\\Laravel\\ButtonDownEmail\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JustSteveKing\\Laravel\\ButtonDownEmail\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "JustSteveKing\\Laravel\\ButtonDownEmail\\ButtonDownServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "guzzlehttp/guzzle": "^7.5",
        "nunomaduro/collision": "^7.0"
    }
}
